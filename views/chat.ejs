    <%- include('partials/header') %>

    <div class="chat-container">
    <div class="chat-panel-wrapper">
        <div class="panel-head">
            <div class="panel-head-flex">
                <div class="panel-head-flex-item"><img src="https://static.remove.bg/remove-bg-web/5c20d2ecc9ddb1b6c85540a333ec65e2c616dbbd/assets/start-1abfb4fe2980eabfbbaaa4365a0692539f7cd2725f324f904565a9a744f8e214.jpg" alt="sss" style="width: 40px; height: 40px;" ></div>
                <div class="panel-head-flex-item">
                    <a href="#">username</a>
                    <label for="cars">Choose an user:</label>

                    <select name="cars" id="cars">
                        <option value="volvo">saurabh</option>
                        <option value="saab">shri kant</option>
                        <option value="mercedes">vivek</option>
                        <option value="audi">John</option>
                    </select>

                </div>
                <div class="panel-head-flex-item align-right">
                    <a href=""><i class="fa-solid fa-xmark"></i></a>
                </div>
            </div>
        </div>
        
        <div class="panel-body">
            <div class="panel-body-content">
                <ul class="chat-table"> </ul>
            </div>
        </div>

        
    </div>
    
    <div class="type-chat-msg">
        <form action="" class="chat-msg-form">
            <div class="msg-input">
                <input type="text" name="chat_msg" id="chat_msg" placeholder="type message">
            </div>
                
            <div class="send-msg">
                <button type="submit"><i class="fa-solid fa-paper-plane"></i></button>
            </div>
        </form>
    </div>

</div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/client.js"></script>
